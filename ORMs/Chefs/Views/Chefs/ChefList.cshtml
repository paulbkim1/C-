<!DOCTYPE html>
<html lang="en">
@model List<Chef>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div>
        <a href="/">Chefs</a>
        <a href="/dishes">Dishes</a>
        <a href="/chefs/new">Add a Chef</a>
        <a href="/dishes/new">Add a Dish</a>
    </div>
    <div>Check out our roster of chefs!</div>
    <div>
        <table>
            <tr>
                <th>Name</th>
                <th>Age</th>
                <th># of Dishes</th>
            </tr>
            @foreach(Chef chef in Model){
            <tr>
                <td>@chef.FirstName @chef.LastName</td>
                <td>@(@DateTime.Today.Year - @chef.DoB.Year)</td>
                <td>@chef.ChefDishs.Count</td>
            </tr>}
        </table>
    </div>
</body>
</html>