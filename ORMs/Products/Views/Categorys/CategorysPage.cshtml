@model List<Category>

<h1><a href="/">Products</a> | <a href="/categories">Categories</a></h1>
<h2>New Category</h2>
@{
    Category NewCategory = new Category();
}
<partial name="_AddCategory" model="@NewCategory"></partial>
<h2>All Categories</h2>
@foreach(Category i in Model)
{
    <p><a href="/categories/@i.CategoryId">@i.Name</a></p>
}